<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">    <modelVersion>4.0.0</modelVersion>    <!-- 子模块引入需要的  -->    <groupId>com.stranger</groupId>    <artifactId>springcloud</artifactId>    <version>1.0</version>    <name>stranger</name>    <description>main</description>    <!--配置版本 -->    <properties>        <project.build.Encoding>UTF-8</project.build.Encoding>        <java.version>1.8</java.version>        <eureka.version>2.2.4.RELEASE</eureka.version>        <spring-boot.version>2.3.3.RELEASE</spring-boot.version>        <spring-cloud.version>Hoxton.SR7</spring-cloud.version>    </properties>    <!--需要公共的模块-->    <dependencies>    </dependencies>    <!--子组件需要的相关依赖-->    <dependencyManagement>        <dependencies>            <!--springboot 需要的pom -->            <dependency>                <groupId>org.springframework.boot</groupId>                <artifactId>spring-boot-dependencies</artifactId>                <version>${spring-boot.version}</version>                <type>pom</type>                <scope>import</scope>            </dependency>            <!-- springcloud需要的pom -->            <dependency>                <groupId>org.springframework.cloud</groupId>                <artifactId>spring-cloud-dependencies</artifactId>                <version>${spring-cloud.version}</version>                <type>pom</type>                <scope>import</scope>            </dependency>            <!-- eureka-server服务 -->            <dependency>                <groupId>org.springframework.cloud</groupId>                <artifactId>spring-cloud-starter-netflix-eureka-server</artifactId>                <version>${eureka.version}</version>            </dependency>            <!-- eureka-client服务 -->            <dependency>                <groupId>org.springframework.cloud</groupId>                <artifactId>spring-cloud-starter-netflix-eureka-client</artifactId>                <version>${eureka.version}</version>            </dependency>            <dependency>                <groupId>com.stranger</groupId>                <artifactId>eureka-common-api</artifactId>                <version>${version}</version>            </dependency>        </dependencies>    </dependencyManagement>    <!--子模块 -->    <modules>        <module>eureka-server</module>        <module>eureka-web</module>        <module>eureka-web-demo</module>        <module>eureka-common-api</module>        <module>eureka-server-demo</module>    </modules>    <!--  *父亲工程  -->    <packaging>pom</packaging>    <build>        <plugins>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-compiler-plugin</artifactId>                <configuration>                    <source>${java.version}</source>                    <target>${java.version}</target>                    <encoding>${project.build.Encoding}</encoding>                </configuration>            </plugin>        </plugins>    </build></project>